node {
        stage('Checkout') {
            git credentialsId: 'job1', url: 'https://github.com/Venkat07m/core-java.git', CredentialsID: 'github', branch: 'master'
        }
        stage('Build') {
            def mvnHome = tool name: 'M2_HOME', type: 'maven'
            sh "${mvnHome}/bin/mvn clean package"

            def pom = readMavenPom file:'ec2-masterclass-sample-app/pom.xml'
            print pom.version
            env.version = pom.version
        }
}
