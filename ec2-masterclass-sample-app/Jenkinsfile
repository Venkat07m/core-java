node {
        stage('Checkout') {
            git credentialsId: 'job1', url: 'https://github.com/Venkat07m/sample-java'
        }
        stage('Build') {
            sh 'mvn clean install'
            def pom = readMavenPom file:'pom.xml'
            print pom.version
            env.version = pom.version
        }
}
